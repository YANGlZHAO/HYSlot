{
  "name": "uniapp-project",
  "version": "1.0.0",
  "description": "A UniApp project",
  "main": "main.js",
  "scripts": {
    "dev:%PLATFORM%": "cross-env NODE_ENV=development UNI_PLATFORM=%PLATFORM% vue-cli-service uni-build --watch",
    "build:%PLATFORM%": "cross-env NODE_ENV=production UNI_PLATFORM=%PLATFORM% vue-cli-service uni-build",
    "serve": "npm run dev:h5",
    "build": "npm run build:h5",
    "build:app-plus": "vue-cli-service uni-build --platform app-plus && node renameApk.js",
    "dev:h5": "cross-env NODE_ENV=development UNI_PLATFORM=h5 vue-cli-service uni-build --watch",
    "build:h5": "vue-cli-service uni-build --platform h5"
  },
  "overrides": {
    "@achrinza/node-ipc": "10.1.11"
  },
  "dependencies": {
    "@dcloudio/uni-ui": "^1.5.8",
    "bootstrap-icons": "^1.13.1",
    "qrcode": "^1.5.4",
    "swiper": "^5.4.5",
    "uqrcodejs": "^4.0.7",
    "uview-ui": "^2.0.38",
    "vue": "^2.6.14",
    "vue-awesome-swiper": "^4.1.1",
    "vue-cat-carousel": "^0.9.3-6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@dcloudio/types": "*",
    "@dcloudio/uni-app": "2.0.2-4070520250711001",
    "@dcloudio/uni-app-plus": "2.0.2-4070520250711001",
    "@dcloudio/uni-cli-i18n": "2.0.2-4070520250711001",
    "@dcloudio/uni-cli-shared": "^2.0.2-4070520250711001",
    "@dcloudio/uni-h5": "2.0.2-4070520250711001",
    "@dcloudio/uni-mp-vue": "2.0.2-4070520250711001",
    "@dcloudio/uni-template-compiler": "2.0.2-4070520250711001",
    "@dcloudio/vue-cli-plugin-uni": "2.0.2-4070520250711001",
    "@dcloudio/webpack-uni-pages-loader": "2.0.2-4070520250711001",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "postcss-pxtorem": "^6.1.0",
    "postcss-rem-to-responsive-pixel": "^6.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "browserslist": [
    "Android >= 4",
    "ios >= 9"
  ]
}
